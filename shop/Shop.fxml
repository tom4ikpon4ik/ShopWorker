            <Font name="Droid Sans" size="12.0" />
         </font></Button>
      <TextField id="BenutzerName" fx:id="BenutzerName" layoutX="50.0" layoutY="44.0" prefHeight="25.0" prefWidth="170.0" />
      <Text layoutX="50.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Benutzername">
         <font>
            <Font name="Droid Sans" size="12.0" />
         </font>
      </Text>
      <Text layoutX="50.0" layoutY="106.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Passwort">
         <font>
            <Font name="Droid Sans" size="12.0" />
         </font>
      </Text>
      <PasswordField fx:id="Passwort" layoutX="50.0" layoutY="109.0" prefWidth="170.0" />
      <Button fx:id="Registrieren" layoutX="140.0" layoutY="154.0" mnemonicParsing="false" prefHeight="16.0" prefWidth="80.0" text="Registrieren" onAction="#handleButton2Action"/>
