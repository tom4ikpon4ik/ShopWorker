<AnchorPane id="AnchorPane" fx:id="Anchor" blendMode="MULTIPLY" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="shop.ShopController">
    
    
    <children>
        <Label fx:id="label" layoutX="14.0" layoutY="6.0" minHeight="16" minWidth="69" text="Wilkommen!" textFill="#513232">
         <font>
            <Font name="Droid Sans" size="12.0" />
         </font></Label>
        <Button fx:id="LogIn" layoutX="49.0" layoutY="154.0" onAction="#handleButton1Action" text="Log In" textAlignment="CENTER">
